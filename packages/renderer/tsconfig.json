{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "noUncheckedIndexedAccess": true,
    "baseUrl": ".",
    "paths": {
      "/@/*": [
        "./src/*"
      ]
    },
    "lib": ["ESNext", "dom", "dom.iterable"]
  },
  "types": [ "ssh2" ],
  "include": [
    "src/**/*.vue",
    "src/**/*.ts",
    "src/**/*.tsx",
    "types/**/*.d.ts",
    "types/*.d.ts",
    "../../types/**/*.d.ts",
    "../preload/types/electron-api.d.ts"
  ]
}
