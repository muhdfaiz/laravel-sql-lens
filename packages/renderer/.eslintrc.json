{
  "env": {
    "browser": true,
    "node": false
  },
  "extends": [
    /** @see https://eslint.vuejs.org/rules/ */
    "plugin:vue/vue3-recommended"
  ],
  "parserOptions": {
    "parser": "@typescript-eslint/parser",
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
      /**
  * This will make the history of changes in the hit a little cleaner
  */
  "comma-dangle": [
    "warn",
    "always-multiline"
  ]
  }

}
