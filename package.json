{
  "name": "laravel-sql-lens",
  "version": "1.0.0",
  "description": "A desktop app built for laravel to monitor, troubleshoot and optimize SQL query.",
  "keywords": [
    "SQL",
    "Slow Query",
    "Laravel",
    "Optimize SQL",
    "Monitor SQL",
    "Duplicate SQL Query SQL",
    "N+1"
  ],
  "engines": {
    "node": ">=v14.16",
    "npm": ">=7.7"
  },
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "build": "node scripts/build.js",
    "precompile": "cross-env MODE=production npm run build",
    "compile": "electron-builder build --config electron-builder.config.js --dir --config.asar=false",
    "clean-dist": "rm -rf ./dist",
    "package:win": "npm run clean-dist && electron-builder --publish never --win --config electron-builder.config.js",
    "package:linux": "npm run clean-dist && electron-builder --publish never --linux --config electron-builder.config.js",
    "package:mac": "npm run clean-dist && electron-builder --publish never --mac --config electron-builder.config.js",
    "pretest": "cross-env MODE=test npm run build",
    "watch": "node scripts/watch.js",
    "lint": "eslint . --ext js,ts,vue",
    "typecheck-main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck-preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck-renderer": "vue-tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck": "npm run typecheck-main && npm run typecheck-preload && npm run typecheck-renderer",
    "postinstall": "electron-builder install-app-deps"
  },
  "browserslist": [
    "Chrome 91"
  ],
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache --fix"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@types/electron-devtools-installer": "2.2.0",
    "@types/node": "^16.9.1",
    "@types/ssh2": "^0.5.48",
    "@typescript-eslint/eslint-plugin": "4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "@vitejs/plugin-vue": "^1.8.0",
    "@vue/compiler-sfc": "^3.2.12",
    "babel-plugin-transform-decorators": "^6.24.1",
    "cross-env": "7.0.3",
    "electron": "^13.4.0",
    "electron-builder": "^22.11.7",
    "electron-devtools-installer": "3.2.0",
    "electron-icon-builder": "^2.0.1",
    "electron-log": "^4.4.1",
    "eslint": "7.32.0",
    "eslint-plugin-vue": "^7.17.0",
    "lint-staged": "11.1.2",
    "regenerator-runtime": "^0.13.9",
    "rollup-plugin-node-builtins": "^2.1.2",
    "sass": "^1.39.2",
    "spectron": "15.0.0",
    "ts-node": "^10.2.1",
    "types": "^0.1.1",
    "typescript": "^4.4.2",
    "vite": "^2.5.10",
    "vue-tsc": "0.3.0",
    "vuex-module-decorators": "^1.0.1"
  },
  "dependencies": {
    "@fontsource/poppins": "^4.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/vue-fontawesome": "^3.0.0-4",
    "@highlightjs/vue-plugin": "^2.0.1",
    "@soerenmartius/vue3-clipboard": "^0.1.2",
    "@types/uniqid": "^5.3.1",
    "@types/vuelidate": "^0.7.15",
    "@vuelidate/core": "^2.0.0-alpha.25",
    "@vuelidate/validators": "^2.0.0-alpha.21",
    "dayjs": "^1.10.7",
    "dexie": "^3.0.3",
    "electron-context-menu": "^3.1.1",
    "electron-updater": "4.6.0",
    "highlight.js": "^11.1.0",
    "primeflex": "^2.0.0",
    "primeicons": "^4.1.0",
    "primevue": "^3.7.2",
    "prismjs": "^1.24.1",
    "sql-formatter": "^4.0.2",
    "ssh2": "^1.4.0",
    "typeface-roboto": "^1.1.13",
    "uniqid": "^5.4.0",
    "vite-plugin-package-version": "^1.0.2",
    "vue": "3.2.11",
    "vue-router": "4.0.11",
    "vuex": "^4.0.2"
  },
  "author": "Muhammad Faiz <muhdfaizhalim@gmail.com>",
  "repository": "https://github.com/muhdfaiz/laravel-sqlens.git",
  "license": "MIT",
  "private": false
}
